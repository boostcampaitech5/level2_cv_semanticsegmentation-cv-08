{
    "base": {
        "use": "smp",
        "pytorch": {
            "model": "deeplabv3_resnet50"
        },
        "smp": {
            "model": "UnetPlusPlus",
            "encoder_name": "resnext101_32x32d",
            "encoder_weights": "instagram"
        }
    },
    "save_model_dir": "/opt/ml/level2_cv_semanticsegmentation-cv-08/model_dir/unet++_resnext101_32x32d_1024",
    "inference_model_dir": "/opt/ml/level2_cv_semanticsegmentation-cv-08/model_dir/unet++_resnext101_32x32d_1024",
    "model_file_name": "best.pth",
    "resume_from": false,
    "dataset": "XRayDatasetFast",
    "image_dir": "/opt/ml/data/train/DCM",
    "label_dir": "/opt/ml/data/train/outputs_json",
    "test_image_dir": "/opt/ml/data/test/DCM",
    "train_json_path": "/opt/ml/data/train_split.json",
    "valid_json_path": "/opt/ml/data/valid_split.json",
    "train_cache_data_dir": "/opt/ml/cache_data_512/train",
    "valid_cache_data_dir": "/opt/ml/cache_data_512/valid",
    "train_hdf5_data_dir": "/opt/ml/hdf5_1280_gray/train",
    "valid_hdf5_data_dir": "/opt/ml/hdf5_1280_gray/valid",
    "epochs": 200,
    "train": {
        "batch_size": 2,
        "num_workers": 8,
        "augmentations": {
            "name": "horizontal_flip",
            "parameters": {
                "resize": 1024,
                "p": 0.5
            }
        }
    },
    "valid": {
        "batch_size": 2,
        "num_workers": 4,
        "augmentations": {
            "name": "base_augmentation",
            "parameters": {
                "resize": 1024
            }
        }
    },
    "test": {
        "batch_size": 1,
        "num_workers": 2,
        "augmentations": {
            "name": "base_augmentation",
            "parameters": {
                "resize": 1024
            }
        }
    },
    "optimizer": {
        "type": "AdamW",
        "parameters": {
            "lr": 1e-5,
            "weight_decay": 1e-06
        }
    },
    "criterion": "BCEDiceLoss",
    "scheduler": {
        "type": "CosineAnnealingWarmRestarts",
        "parameters": {
            "T_0": 5,
            "T_mult": 1,
            "eta_min": 1e-06
        }
    },
    "seed": 42,
    "log_step": 20,
    "val_every": 1,
    "patience_limit": 20,
    "accumulation_step": 1,
    "fp16": true,
    "wandb": {
        "use": true,
        "entity": "ejrtks1020",
        "project": "level2_segmentation",
        "run_name": "unet++_resnext101_32x32d_1024",
        "id": false,
        "resume": false
    },
    "gray": true,
    "validation": true,
    "copy_paste": {
        "k": 3
    },
    "threshold": 0.5
}