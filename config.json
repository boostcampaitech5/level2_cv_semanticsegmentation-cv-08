{
    "base": {
        "use": "smp",

        "pytorch": {
            "model": "hrnet"
        },
        "smp": {
            "model": "UnetPlusPlus",
            "parameters" : {
                "encoder_name": "swin_encoder",
                "encoder_weights": "imagenet",
                "encoder_depth": 4,
                "decoder_channels": [512, 256, 128, 64]
            }
        }
    },
    "save_model_dir": "/opt/ml/input/level2_cv_semanticsegmentation-cv-08/model_dir/test",
    "inference_model_dir": "/opt/ml/input/level2_cv_semanticsegmentation-cv-08/model_dir/test",
    "model_file_name": "best.pth",
    "resume_from": false,
    "dataset": "XRayDatasetFast",
    "image_dir": "/opt/ml/input/data_imglabel/train/DCM",
    "label_dir": "/opt/ml/input/data_imglabel/train/outputs_json",
    "test_image_dir": "/opt/ml/input/data/test/DCM",
    "train_json_path": "/opt/ml/input/data/train_split.json",
    "valid_json_path": "/opt/ml/input/data/valid_split.json",
    "train_cache_data_dir": "/opt/ml/input/cache_data_512/train",
    "valid_cache_data_dir": "/opt/ml/input/cache_data_512/valid",
    "train_hdf5_data_dir": "/opt/ml/input/hdf5_data_512/train",
    "valid_hdf5_data_dir": "/opt/ml/input/hdf5_data_512/valid",
    "epochs": 150,
    "train": {
        "batch_size": 2,
        "num_workers": 2,
        "augmentations": {
            "name": "flipbrightness",
            "parameters": {
                "resize": 512
            }
        }
    },
    "valid": {
        "batch_size": 2,
        "num_workers": 2,
        "augmentations": {
            "name": "base_augmentation",
            "parameters": {
                "resize": 512
            }
        }
    },
    "test": {
        "batch_size": 2,
        "num_workers": 2,
        "augmentations": {
            "name": "base_augmentation",
            "parameters": {
                "resize": 512
            }
        }
    },
    "optimizer": {
        "type": "Adam",
        "parameters": {
            "lr": 0.0001,
            "weight_decay": 1e-06
        }
    },
    "criterion": "BCEDiceLoss",
    "scheduler": {
        "type": "MultiStepLR",
        "parameters": {
            "milestones": [
                150
            ],
            "gamma": 0.5
        }
    },
    "seed": 42,
    "log_step": 20,
    "val_every": 1,
    "patience_limit": 20,
    "accumulation_step": 1,
    "fp16": true,
    "wandb": {
        "use": false,
        "entity": "lijm1358",
        "project": "level2_segmentation",
        "run_name": "unetpp_swinL"
    }
}