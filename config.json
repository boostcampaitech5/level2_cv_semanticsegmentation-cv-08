{
    "base": {
        "use": "smp",

        "pytorch": {
            "model": "hrnet"
        },
        "smp": {
            "model": "UnetPlusPlus",
            "encoder_name": "resnet50",
            "encoder_weights": "imagenet"
        }
    },

    "save_model_dir": "/opt/ml/input/results/hrnet",
    "inference_model_dir": "/opt/ml/input/results/hrnet",
    "model_file_name": "best.pth",
    "resume_from": false,

    "dataset": "Hdf5Dataset",

    "image_dir": "/opt/ml/input/data/train/DCM",
    "label_dir": "/opt/ml/input/data/train/outputs_json",
    "test_image_dir": "/opt/ml/input/data/test/DCM",

    "train_json_path": "/opt/ml/input/data/train_split.json",
    "valid_json_path": "/opt/ml/input/data/valid_split.json",

    "train_cache_data_dir": "/opt/ml/input/cache_data_512/train",
    "valid_cache_data_dir": "/opt/ml/input/cache_data_512/valid",

    "train_hdf5_data_dir": "/opt/ml/input/hdf5_data_512/train",
    "valid_hdf5_data_dir": "/opt/ml/input/hdf5_data_512/valid",

    "epochs": 1,

    "train": {
        "batch_size": 8,
        "num_workers": 4,
        "augmentations": {
            "name": "base_augmentation",
            "parameters": {
                "resize": 512
            }
        }
    },
    "valid": {
        "batch_size": 4,
        "num_workers": 2,
        "augmentations": {
            "name": "base_augmentation",
            "parameters": {
                "resize": 512
            }
        }
    },
    "test": {
        "batch_size": 1,
        "num_workers": 8,
        "augmentations": {
            "name": "base_augmentation",
            "parameters": {
                "resize": 512
            }
        }
    },

    "optimizer": {
        "type": "Adam",
        "parameters": {
            "lr": 0.0001,
            "weight_decay": 1e-06
        }
    },
    "criterion": "BCEWithLogitsLoss",
    "scheduler": {
        "type": "MultiStepLR",
        "parameters": {
            "milestones": [
                50,
                100
            ],
            "gamma": 0.5
        }
    },

    "seed": 42,
    "log_step": 20,
    "val_every": 1,
    "patience_limit": 20,
    "accumulation_step": 1,
    "fp16": true,
    "wandb": {
        "use": false,
        "entity": "ejrtks1020",
        "project": "level2_segmentation",
        "run_name": "unet_regnety-040_512_epoch-15-start"
    }    
}